Dude.prototype.update = function() {
        if (this.currentSpeed > 0)
        {
            this.currentSpeed -= 7;
        }
        // check if player is standing on teleporter 1
        if (teleportplayer1(this.dude)) {
          this.dude.body.x = 2680;
          this.dude.body.y = 230;
        }
        this.dude.body.velocity.x = 0;
        this.dude.body.velocity.y = 0;
};

function update() {

    for (var i in playersList) {
		  if (!playersList[i]) continue;
	  	var curDude = playersList[i].dude;
		  for (var j in playersList) {
			  if (!playersList[j]) continue;
			  if (playersList[j].alive) {
				  playersList[j].update();
			  }
		}
}
